{
  "manifest_version": 2,

  "name": "Titanic",
  "description": "Prevent the syncing of blacklisted SKUs in Hybris",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Titanic",
    "default_popup": "index.html"
  },
  "permissions": [
      "activeTab",
      "<all_urls>"
  ] 
}